.table{border-collapse: collapse;
}







.pagination {
.disabled {
display: none;
}

background: white;
cursor: default;
height: 22px;
a, span, em {
padding: 0.2em 0.5em;
display: block;
float: left;
margin-right: 1px;
}
.disabled {
color: #999999;
border: 1px solid #dddddd;
}
.current {
font-style: normal;
font-weight: bold;
background: #40bf4c;
color: #21953d;
border: 1px solid #40bf4c;
}
a{
text-decoration: none;
color: #40bf4c;
border: 1px solid #6ae275;
&:hover, &:focus {
color: #000033;
border-color: #000033;
}
}
.page_info {
