<div class="navbar navbar-default " role="navigation">
  <div class="container-fluid">
    <div class="navbar-header">
      <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
      <%= link_to 'GanjaMap', home_path, class: 'navbar-brand' %>
    </div>
    <div class="collapse navbar-collapse navbar-right">
      <ul class="nav navbar-nav">
        <% if signed_in? %>

            <li><%= link_to "Profile", current_user %></li>

            <li><a href="#">Publications</a></li>

            <li><%= link_to signout_path, method: "delete" do %>
                  <i class="fa fa-sign-out"></i>
              <% end %>
            </li>
        <% else %>

            <li>
              <%= link_to signin_path, id: "Sign in" do %>
                  <i class="fa fa-user"></i>
              <% end %>
            </li>
        <% end %>
      </ul>
    </div>

  </div>
</div>
<div class="container navbar-collapse collapse nav-bottom">
  <%= form_tag(@ganjaloc, method: "get", role: "search", class: "navbar-form navbar") do %>

      <%= search_field_tag :search, params[:search], placeholder: "Search", class: "form-control" %>

  <% end %>
</div>

